package cn.i.learn.scala.foldleft

object Main {
    def main(args: Array[String]): Unit = {
        val list = "1" :: "2" :: "3" :: Nil
        list.foreach(println(_))
        println("___")
        println(list.foldLeft("A")((total, value) => {
            println("total:" + total)
            total + value
        }))
        println("___")
        println(list.foldRight("A")((total, value) => {
            println("total:" + total)
            total + value
        }))
    }
}