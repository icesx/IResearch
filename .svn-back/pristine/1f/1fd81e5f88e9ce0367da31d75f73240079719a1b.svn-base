package cn.i.learn.scala.base

object Reduce {
    def main(args: Array[String]): Unit = {
        println(List("1", "1", "3", "5", "3").reduce((s1, s2) => s1 + s2))
        println(List("1").reduce((s1, s2) => {
            println(s1+"=")
            println(s2+"=")
            s1
        }))
    }
}