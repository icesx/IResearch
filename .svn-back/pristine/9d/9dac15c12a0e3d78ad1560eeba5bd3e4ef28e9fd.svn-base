package com.javapatterns.command.javaawt;

import java.awt.*;

public class LetThereBeLandCommand extends Button implements CommandFromGod
{
    private Panel p;

    public LetThereBeLandCommand(String caption, Panel pnl)
    {
        super(caption);
        p = pnl;
    }

    public void execute()
    {
        p.setBackground(Color.orange);
    }
}

