package cn.i.learn.scala.obj.sington

class ApplySingtion {
  private val name = "apply"
  def say() = {
    println(name)
  }
}
object ApplySington {
  private val as: ApplySingtion = new ApplySingtion
  def apply() = {
    as
  }

  def main(args: Array[String]): Unit = {
    ApplySington().say()
  }
}

