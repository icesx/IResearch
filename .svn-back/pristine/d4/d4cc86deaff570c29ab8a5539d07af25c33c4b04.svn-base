package cn.i.learn.scala.base

object ArraySample {
    def sampleArray() = {
        val greetStrings = new Array[String](3)
        greetStrings(0) = "Hello"
        greetStrings(1) = ", "
        greetStrings(2) = "world!\n"
        greetStrings.+:("end")
        greetStrings.foreach { println }
    }
    def main(args: Array[String]): Unit = {
        sampleArray()
    }
}